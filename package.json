{
  "name": "api_nodejs",
  "version": "1.0.0",
  "description": "Desenvolvimento Inicial API REST NODEJS",
  "main": "dist/entry.js",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "dev": "tsc && nodemon -r dotenv/config --inspect ./dist/entry.js",
    "start:dev": "nodemon --watch 'app/' --exec 'ts-node src/index.ts' -e ts",
    "start": "npm run build && nodemon ./dist/entry.js",
    "prod": "npm run build && npm run start",
    "debug": "npm run build && npm run watch-debug",
    "postgres:start": "docker run -d --name node_postgresql -e POSTGRES_USER=admin -e POSTGRES_PASSWORD=123456 -e POSTGRES_DB=postgres -v %cd%/pgdata:/var/lib/postgresql/data -p 5432:5432 postgres",
    "postgres:stop": "docker stop node_postgresql && docker rm node_postgresql",
    "pgadmin:start": "docker run -d --name pgadmin_container -p 8080:80 -e PGADMIN_DEFAULT_EMAIL=admin@admin.com -e PGADMIN_DEFAULT_PASSWORD=123456 -v %cd%/backup:/var/lib/postgresql/data dpage/pgadmin4",
    "pgadmin:stop": "docker stop pgadmin_container && docker rm pgadmin_container",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest",
    "teste-unico": "jest test/controller/farm.controller.test.ts",
    "test:autocannon": "npx autocannon -c 500 -d 30 --workers 16 --renderStatusCodes --latency --warmup [-c 1 -d 2] http://localhost:3300"
  },
  "keywords": [
    "API",
    "REST",
    "NODE"
  ],
  "author": "Thiago de Carlo Goncalves",
  "license": "MIT   ",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "sequelize": "^6.15.1",
    "sequelize-cli": "^6.6.1",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.8.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.1",
    "@types/pg": "^8.10.2",
    "@types/sequelize": "^4.28.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autocannon": "^7.11.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
