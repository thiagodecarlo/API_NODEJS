{
  "name": "api_nodejs",
  "version": "1.0.0",
  "description": "Desenvolvimento Inicial API REST NODEJS",
  "main": "dist/entry.js",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "dev": "tsc && nodemon -r dotenv/config --inspect ./dist/entry.js",
    "start:dev": "nodemon --watch 'app/' --exec 'ts-node src/index.ts -e ts",
    "start": "npm run build && nodemon ./dist/entry.js",
    "prod": "npm run build && npm run start",
    "debug": "npm run build && npm run watch-debug",
    "start:docker": "docker-compose up -d",
    "down:docker": "docker-compose down",
    "test": "npx autocannon -c 500 -d 30 --workers 16 --renderStatusCodes --latency --warmup [-c 1 -d 2] http://localhost:3300"
  },
  "keywords": [
    "API",
    "REST",
    "NODE"
  ],
  "author": "Thiago de Carlo Goncalves",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.1",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autocannon": "^7.11.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
